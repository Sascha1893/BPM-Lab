                    <bpel:if name="IFproduktnichtvorhanden">
                        <bpel:condition><![CDATA[$CheckVorhandenResponse.checkAvailabilityOutput/ns:product/ns:status/ns:currentAvailability ="not available"]]></bpel:condition>
                        <bpel:sequence>
                            
                            
                            <bpel:throw name="Throw" faultName="tns:ProductNichtVorrÃ¤tig" faultVariable="CheckVorhandenResponse"></bpel:throw>
                            
                        </bpel:sequence>
                    </bpel:if>
                </bpel:sequence>
            </bpel:scope>
        </bpel:forEach>
        <bpel:assign validate="no" name="AssignStoreOrder">
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
