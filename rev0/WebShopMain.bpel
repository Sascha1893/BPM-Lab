            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeStoreOrder" partnerLink="OrderServierPartner" operation="storeOrderDetails" portType="ns:OrderPortType" inputVariable="StoreOrderRequest" outputVariable="StoreOrderResponse"></bpel:invoke>
        
        
        
        <bpel:reply name="replyOutput" partnerLink="client" portType="tns:WebShopMain" operation="process" variable="outputOrder" />
    
